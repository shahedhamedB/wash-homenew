<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MultiStateView">
        <attr name="msv_loadingView" format="reference" />
        <attr name="msv_emptyView" format="reference" />
        <attr name="msv_errorView" format="reference" />
        <attr name="msv_viewState" format="enum">
            <enum name="content" value="0" />
            <enum name="error" value="1" />
            <enum name="empty" value="2" />
            <enum name="loading" value="3" />
        </attr>
        <attr name="msv_animateViewChanges" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SwipeButtonView">
        <attr name="sb_swipe_bg_color" format="color" />
        <attr name="sb_thumb_bg_color" format="color" />
        <attr name="sb_stroke_bg_color" format="color" />
        <attr name="sb_thumb_image" format="reference" />
        <attr name="sb_swipe_text" format="string" />
        <attr name="sb_swipe_text_color" format="color" />
        <attr name="sb_swipe_animate_text" format="boolean" />
        <attr name="sb_swipe_text_size" format="dimension" />
        <attr name="sb_swipe_reverse" format="boolean" />
        <attr name="sb_swipe_both_direction" format="boolean" />
    </declare-styleable>
</resources>